{
  "permissions": {
    "allow": [
      "WebFetch",
      "WebSearch",
      "Bash(./gradlew:*)",
      "Bash(git ls-remote:*)",
      "Bash(git log:*)",
      "Bash(git diff:*)",
      "Bash(git status:*)",
      "Bash(gh run list:*)",
      "Bash(gh run view:*)",
      "Bash(gh pr list:*)",
      "Bash(gh pr view:*)",
      "Bash(gh repo view:*)",
      "Bash(gh issue view:*)"
    ],
    "deny": [
      "Edit(./**/*.lockfile)",
      "Edit(./gradle/verification-metadata.xml)",
      "Write(./**/*.lockfile)",
      "Write(./gradle/verification-metadata.xml)"
    ],
    "ask": ["Bash(gh api:*)"]
  }
}
